<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    card_view:cardCornerRadius="4dp"
    card_view:cardElevation="0dp"
    android:layout_marginBottom="2dp"
    android:id="@+id/cardview_list_kartu_keluarga_search">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingHorizontal="20dp"
            android:paddingVertical="20dp">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/textview_search_kartu_keluarga_nokk"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                card_view:layout_constraintTop_toTopOf="parent"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                android:textSize="16sp"
                android:text="1382038172312"
                />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/textview_search_kartu_keluarga_nama"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                card_view:layout_constraintTop_toBottomOf="@id/textview_search_kartu_keluarga_nokk"
                card_view:layout_constraintStart_toStartOf="parent"
                card_view:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="4dp"
                android:textSize="16sp"
                android:text="Hanan"
                android:textStyle="bold"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>
